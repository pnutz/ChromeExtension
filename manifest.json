{
  "manifest_version": 2,

  "name": "TwoReceipt",
  "description": "This extension demonstrates a 'browser action' with NIGGAS.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://localhost:3000/api/v1/tokens.json"
  ]
}
